Pseudo2 = function(N,x0){
a=7^7;
b=2^31-1
x=c(1:N);
X=c(1:N)
x[1]=x0;
X[1]=(x0%%b)/b
for (j in c(1:(N-1)))
{
x[j+1]=(a*x[j]+1)%%b
X[j+1]=x[j+1]/b
}
return(X)
}
X=Pseudo2(1000,123456);
hist(X)
rm(list = ls(all = TRUE))
